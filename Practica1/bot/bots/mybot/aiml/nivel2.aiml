                    </set>
                    <set var="item"><first><get var="list"/></first></set>
                    <set var="list"><rest><get var="list"/></rest></set>
                    <set var="item"><first><get var="list"/></first></set>
                    <set var="list"><rest><get var="list"/></rest></set>
                    <loop/>
                </li>
